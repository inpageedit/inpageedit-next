{
  "name": "@inpageedit/core",
  "version": "0.9.0",
  "description": "ðŸš€ Modular, Extensible Supercharged Plugin for MediaWiki",
  "author": "dragon-fish",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./InPageEdit": {
      "source": "./src/InPageEdit.ts",
      "types": "./dist/InPageEdit.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./models/*": {
      "types": "./dist/plugins/*.d.ts"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts"
    },
    "./types/*": {
      "types": "./dist/services/*.d.ts"
    },
    "./src/*": {
      "source": "./src/*",
      "types": "./src/*"
    },
    "./dist/*": {
      "source": "./dist/*",
      "types": "./dist/*"
    }
  },
  "scripts": {
    "start": "zx dev/scripts/setup-dev.ts",
    "dev": "vite",
    "test": "vitest run ./src",
    "build": "run-p build:*",
    "build:import": "cross-env VITE_BUILD_FORMAT=import vite build",
    "build:bundle": "cross-env VITE_BUILD_FORMAT=bundle vite build",
    "prepublish": "npm run build",
    "lint": "eslint ./src",
    "unit-test": "vitest run ./src",
    "unit-test:watch": "vitest ./src",
    "clean": "rimraf dist/* lib/*",
    "format": "prettier --write ./src",
    "typecheck": "tsc --noEmit --project ./tsconfig.app.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@microsoft/api-extractor": "^7.53.3",
    "@prettier/plugin-pug": "^3.4.2",
    "@tabler/icons-vue": "^3.35.0",
    "@types/node": "^22.18.13",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/language-plugin-pug": "^3.1.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jsx-dom": "^8.1.6",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "pug": "^3.0.3",
    "rimraf": "^6.1.0",
    "sass-embedded": "^1.93.2",
    "tsx": "^4.20.6",
    "types-mediawiki": "^2.0.0",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.1.12",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@cordisjs/core": "^3.18.1",
    "@inpageedit/logger": "^0.1.0",
    "@inpageedit/modal": "workspace:^",
    "idb-keyval": "^6.2.2",
    "localforage": "^1.10.0",
    "schemastery": "^3.17.1",
    "schemastery-form": "workspace:^",
    "vue": "^3.5.22",
    "wiki-saikou": "^7.1.2"
  },
  "keywords": [
    "mediawiki",
    "mediawiki-gadget",
    "inpageedit"
  ],
  "homepage": "https://www.ipe.wiki/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inpageedit/inpageedit-next.git"
  },
  "bugs": {
    "url": "https://github.com/inpageedit/inpageedit-next/issues"
  }
}