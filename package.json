{
  "name": "@inpageedit/core",
  "version": "0.1.1",
  "description": "ðŸš€ Modular, Extensible Supercharged Plugin for MediaWiki",
  "author": "dragon-fish",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./InPageEdit": {
      "types": "./dist/InPageEdit.d.ts"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./models/*": {
      "types": "./dist/plugins/*.d.ts"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts"
    },
    "./types/*": {
      "types": "./dist/services/*.d.ts"
    }
  },
  "scripts": {
    "start": "zx dev/scripts/setup-dev.ts",
    "dev": "vite",
    "build": "run-p build:*",
    "build:import": "cross-env VITE_BUILD_FORMAT=import vite build",
    "build:bundle": "cross-env VITE_BUILD_FORMAT=bundle vite build",
    "prepublish": "npm run build",
    "lint": "eslint ./src",
    "clean": "rimraf dist/* lib/*",
    "format": "prettier --write ./src",
    "doc:dev": "vitepress dev docs",
    "doc:build": "npm run build:import && vitepress build docs",
    "doc:preview": "vitepress preview docs",
    "typecheck": "tsc -b --pretty false --noEmit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@inpageedit/core": "workspace:^",
    "@microsoft/api-extractor": "^7.52.13",
    "@nolebase/vitepress-plugin-git-changelog": "^2.18.2",
    "@nolebase/vitepress-plugin-inline-link-preview": "^2.18.2",
    "@nolebase/vitepress-plugin-page-properties": "^2.18.2",
    "@prettier/plugin-pug": "^3.4.2",
    "@shikijs/vitepress-twoslash": "^3.13.0",
    "@tabler/icons-vue": "^3.35.0",
    "@types/jquery": "^3.5.33",
    "@types/node": "^22.18.6",
    "@types/ssi-modal": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/language-plugin-pug": "^3.0.8",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "fexios": "^3.1.0",
    "globals": "^16.4.0",
    "jsx-dom": "^8.1.6",
    "markdown-it-task-lists": "^2.1.1",
    "mermaid": "^11.12.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "pug": "^3.0.3",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.93.2",
    "tsx": "^4.20.5",
    "types-mediawiki": "^2.0.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.1.7",
    "vitepress": "2.0.0-alpha.12",
    "vitepress-i18n": "^1.3.4",
    "vitepress-markdown-timeline": "^1.2.2",
    "vitepress-plugin-group-icons": "^1.6.3",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitepress-sidebar": "^1.33.0",
    "zx": "^8.8.3"
  },
  "dependencies": {
    "cordis": "^3.18.1",
    "diff": "^8.0.2",
    "localforage": "^1.10.0",
    "naive-ui": "^2.43.1",
    "schemastery": "^3.17.0",
    "vue": "^3.5.21",
    "wiki-saikou": "^5.0.0"
  },
  "peerDependencies": {
    "fexios": "^3.1.0"
  },
  "keywords": [
    "mediawiki",
    "mediawiki-gadget",
    "inpageedit"
  ],
  "homepage": "https://www.ipe.wiki/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inpageedit/inpageedit-next.git"
  },
  "bugs": {
    "url": "https://github.com/inpageedit/inpageedit-next/issues"
  }
}